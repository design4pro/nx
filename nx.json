{
  "npmScope": "design4pro",
  "affected": { "defaultBase": "main" },
  "implicitDependencies": {
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "nx-custom-webpack": { "tags": ["webpack"] },
    "nx-custom-webpack-e2e": {
      "tags": ["webpack"],
      "implicitDependencies": ["nx-custom-webpack"]
    },
    "nx-linter": { "tags": ["linter"] },
    "nx-linter-e2e": { "tags": [], "implicitDependencies": ["nx-linter"] },
    "nx-strapi": { "tags": ["strapi"] },
    "nx-strapi-e2e": { "tags": [], "implicitDependencies": ["nx-strapi"] }
  },
  "workspaceLayout": { "appsDir": "e2e", "libsDir": "packages" }
}
