{
  "$schema": "http://json-schema.org/schema",
  "name": "Semantic Release",
  "version": "0.0.1",
  "generators": {
    "init": {
      "factory": "./src/generators/init/init",
      "schema": "./src/generators/init/schema.json",
      "description": "Initialize @design4pro/nx-release plugin",
      "aliases": ["ng-add"]
    },
    "add": {
      "factory": "./src/generators/add/add",
      "schema": "./src/generators/add/schema.json",
      "description": "Add Semantic Release configuration to project"
    }
  },
  "schematics": {
    "init": {
      "factory": "./src/generators/init/init.schematics",
      "schema": "./src/generators/init/schema.json",
      "description": "Initialize @design4pro/nx-release plugin",
      "aliases": ["ng-add"]
    }
  }
}
